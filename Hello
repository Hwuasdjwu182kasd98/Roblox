-- Library
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Collecting Simulator", "Ocean")
--Tabs
local Credits = Window:NewTab("Credits")
local Farming = Window:NewTab("Farming")
local AutoEgg = Window:NewTab("Eggs")
local Teleports = Window:NewTab("Teleport")
local Settings = Window:NewTab("Settings")
--Sections
local CreditsS = Credits:NewSection("Click To Copy")
local AutoFarmS = Farming:NewSection("AutoFarm")
local FarmingS = Farming:NewSection("Inf Backpack")
local AntiAfkS = Farming:NewSection("ANTI AFK")
local AutoEggS = AutoEgg:NewSection("Eggs")
local TeleportsS = Teleports:NewSection("Teleports")
local SettingsS = Settings:NewSection("Settings")
--Functions

function TeleportTO(placeCFrame)
    local plyr = game.Players.LocalPlayer
    if plyr.Character then
        plyr.Character.HumanoidRootPart.CFrame = placeCFrame;
    end
end

--Buttons.

CreditsS:NewButton("Unknown", "Click To Copy", function()
    setclipboard("Unknown")
end)

AntiAfkS:NewButton("AntiAFK", "", function()
    local vu = game:GetService("VirtualUser")
    game:GetService("Players").LocalPlayer.Idled:connect(function()
        vu:Button2Down(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
        wait(1)
        vu:Button2Up(Vector2.new(0,0),workspace.CurrentCamera.CFrame)
    end)
end)

FarmingS:NewButton("Click", "", function()
    game:GetService("Players").LocalPlayer.BackpackData.MaxStorage.Value = 10000000000000000000
end)

local Eggs = {"Common Egg", "[G] Common Egg", "Farm Egg", "[G] Farm Egg", 
              "Jungle Egg", "[G] Jungle Egg", "Release Egg"}
local Buy = {"Buy1", "Buy3", "Buy6"}

AutoEggS:NewDropdown("Choose Egg", "YaY", Eggs, function(EggsCount)
    getgenv().EggsCount1 = EggsCount
    print(EggsCount1)
end)
AutoEggS:NewDropdown("Choose How Much Buy", "YaY", Buy, function(BuyCount)
    getgenv().BuyCount1 = BuyCount
    print(BuyCount1)
end)

AutoEggS:NewToggle("Start", "Start The Automaticly Buy Egg", function(StartEggs)
    if StartEggs then
        getgenv().StartEggs1 = true
        while wait(0.1) and getgenv().StartEggs1 do
        game:GetService("ReplicatedStorage"):WaitForChild("GameClient"):WaitForChild("Events"):WaitForChild("RemoteFunction"):WaitForChild("BuyEgg"):InvokeServer(EggsCount1, BuyCount1)
        end
    else
        getgenv().StartEggs1 = false
    end
end)

function teleportEgg(Eggs)
    if game:GetService("Workspace").Eggs:FindFirstChild(Eggs) then
        TeleportTO(game:GetService("Workspace").Eggs[Eggs].Sign.CFrame)
    end
end

AutoEggS:NewButton("Teleport To Egg", "ButtonInfo", function()
    if EggsCount1 then
        teleportEgg(EggsCount1)
    end
end)

function teleportWorld(Part)
    if game:GetService("Workspace").TeleSpawns:FindFirstChild(Part) then
        TeleportTO(game:GetService("Workspace").TeleSpawns[Part].CFrame)
    end
end

local Teleportdaw = {"City","Farm","Jungle"}

TeleportsS:NewDropdown("Select a map", "YaY", Teleportdaw, function(value)
    selectedWorld = value
    print(value)
end)

TeleportsS:NewButton("Click To Teleport", "ButtonInfo", function()
    if selectedWorld then
        teleportWorld(selectedWorld)
    end
end)

AutoFarmS:NewToggle("AutoFarm Cash", "ToggleInfo", function(state)
    if state then
        local clickdetector = game:GetService("Workspace").Items["Items/Farm"].Wheat.Wheat.ClickDetector
        local clickdetector2 = game:GetService("Workspace").Items["Items/City"].Pot.Pot.ClickDetector
        local clickdetector3 = game:GetService("Workspace").Items["Items/City"].Tire.Tire.ClickDetector
        local clickdetector4 = game:GetService("Workspace").Items["Items/Jungle"].Acorn["Meshes/Acorn"].ClickDetector
        local numClicks = 8
        getgenv().Start = true;
        
        while wait() and getgenv().Start do
            
            for i = 1, numClicks do
                fireclickdetector(clickdetector)
                fireclickdetector(clickdetector2)
                fireclickdetector(clickdetector3)
                fireclickdetector(clickdetector4)
                game:GetService("Players").LocalPlayer.Hidden.DMG.Value = 0
                local playerHead = game.Players.LocalPlayer.Character.HumanoidRootPart
                for i, v in pairs(game:GetService("Workspace").City.SELL["Sell Place"]:GetDescendants())do
                    if v.Name == "TouchInterest" and v.Parent then
                        firetouchinterest(playerHead, v.Parent, 0)
                        wait(0.01)
                        firetouchinterest(playerHead, v.Parent, 1)
                    end
                end
                wait(0.1)
            end
        end
    else
        getgenv().Start = false;
    end
end)

SettingsS:NewKeybind("ToggleUI", "KeybindInfo", Enum.KeyCode.LeftAlt, function()
	Library:ToggleUI()
end)
